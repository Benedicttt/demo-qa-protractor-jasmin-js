{
  "name": "smoke",
  "version": "1.0.0",
  "description": "",
  "main": "conf.js",
  "dependencies": {
    "angular": "^1.7.3",
    "chrome": "^0.1.0",
    "jasmine-allure-reporter": "^1.0.2",
    "jasmine-spec-reporter": "^4.2.1",
    "jquery": "^3.3.1",
    "js-yaml": "^3.12.0",
    "protractor": "^5.4.0",
    "protractor-http-client": "^1.0.4",
    "webdriver": "^5.0.0-alpha.7"
  },
  "devDependencies": {
    "@types/jquery": "^3.3.6",
    "allure-commandline": "^2.7.0",
    "grunt-protractor-runner": "^5.0.0",
    "parallel-protractor": "^1.0.0"
  },
  "scripts": {
    "chrome": "protractor conf.js  --browser chrome --continue-on-error --disableChecks",
    "chrome and report": "protractor conf.js  --browser chrome --continue-on-error --disableChecks && allure generate allure-results --clean -o allure-report && allure serve ./allure-results",
    "firefox": "protractor conf.js  --browser firefox",
    "firefox_headless": "MOZ_HEADLESS=1 protractor conf.js  --browser firefox",
    "webdriver-update": "node_modules/protractor/bin/webdriver-manager update",
    "trouble": "protractor --troubleshoot true conf.js --browser 'chrome'",
    "allure server ./allure-result": "allure generate allure-results --clean -o allure-report && allure serve ./allure-results",
    "exit": "exit 0",
    "delete folder ./allure-report": "rm -rf allure-report allure-results"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.binomo.com/finances/smoke.git"
  },
  "author": "Sergey Kievskiy",
  "license": "404"
}
